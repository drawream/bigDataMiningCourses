lagrange<-function(x,xk,yk){
n<-length(xk) #n为列的长度
lagr<-0
if(length(xk)!=length(yk))
stop("the length of input values xk and yk is not equal!")
if(n<2)
stop("the length of xk and yk should be bigger than 1")
for(i in 1:n)
{
Li<-1
for(j in 1:n)
{
if(i!=j)
Li<-Li*(x-xk[j])/(xk[i]-xk[j])
}
lagr<-Li*yk[i]+lagr
}
return(lagr)
}
